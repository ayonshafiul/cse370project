<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Cyber Cars</title>
</head>
<body>
    <div class="nav">
        <div class="logo">Cyber Cars</div>
        <ul>
            <li><a href="/">Home</a></li>
            <li><a class="active" href="/login">Login</a></li>
            <li><a href="/register">Register</a></li>
        </ul>
    </div>
    <div class="container">
        <div class="table-data">
            <h2>Search Results</h2>
            <table>
                <tr>
                    <th>Manufacturer</th>
                    <th>Model</th>
                    <th>Color</th>
                    <th>Price</th>
                </tr>
                <% for(var i = 0; i < data.length; i++) { %>
                    <tr>
                    <td> <%= data[i].name %></td>
                   <td><a href="/"><%= data[i].model %></a> </td>
                   <td> <%= data[i].color %> </td>
                    <td> <%= data[i].price %></td>
                    </tr>
                <% } %>
        </table>
        </div>
    </div>
    
</body>
<script>
    function ConfirmDelete()
    {
      var x = confirm("Are you sure you want to delete?");
      if (x)
          return true;
      else
        return false;
    }
</script> 
</html>