<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Cyber Cars</title>
</head>
<body>
    <%- include('partials/navbar', {
        nav: {
            login: '/admin/login',
            addCar: '/admin/car',
            addMnfctr: '/admin/manufacturer',
            cars: '/admin/car/update',
            manufacturers: '/admin/manufacturer/update',
            search: '/admin/search',
            users: '/admin/customerDetails',
            messages: '/admin/message',
            status: '/admin/carstatus'
        },
        active: 'cars'
    }) %>
    <div class="container">
        <ul>
            <% for(var i = 0; i < data.length; i++) { %>
               <li> <%= data[i].color %> <%= data[i].model %> <%= data[i].price %> <%= data[i].stock %> <%= data[i].engine %> <%= data[i].rangeKM %> <%= data[i].braking %> <%= data[i].maxSpeed %> <%= data[i].autoPilot %> <%= data[i].cameras %> <%= data[i].manufacturerID %> <a href="/admin/car/update/<%= data[i].carID %>">Edit </a><a href="/admin/car/delete/<%= data[i].carID %>">Delete</a></li> 
            <% } %>
        </ul>
    </div>
</body>
</html>