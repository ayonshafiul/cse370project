<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Cyber Cars</title>
</head>
<body>
    <div class="nav">
        <div class="logo">Cyber Cars</div>
        <ul>
            <li><a href="/">Home</a></li>
            <li><a class="active" href="/login">Login</a></li>
            <li><a href="/register">Register</a></li>
        </ul>
    </div>
    <div class="container">
        <ul>
            <% for(var i = 0; i < data.length; i++) { %>
               <li> <%= data[i].color %> <%= data[i].model %> <%= data[i].price %> <%= data[i].stock %> <%= data[i].engine %> <%= data[i].rangeKM %> <%= data[i].braking %> <%= data[i].maxSpeed %> <%= data[i].autoPilot %> <%= data[i].cameras %> <%= data[i].manufacturerID %> <a href="/admin/car/update/<%= data[i].carID %>">Edit </a><a href="/admin/car/delete/<%= data[i].carID %>">Delete</a></li> 
            <% } %>
        </ul>
    </div>
</body>
</html>